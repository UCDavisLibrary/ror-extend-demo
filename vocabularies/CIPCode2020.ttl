cip:22.0224  rdf:type       skos:Concept ;
        skos:prefLabel      "Tribal/Indigenous Law."@en ;
        skos:notation       "22.0224" ;
        skos:broader        cip:22.02 ;
        schema:description  "An advanced, professional study of the law, policies, and regulations affecting federal Indian law, tribal law and policy, and Indigenous peoples' human rights."@en .

cip:22.0299  rdf:type       skos:Concept ;
        skos:prefLabel      "Legal Research and Advanced Professional Studies, Other."@en ;
        skos:notation       "22.0299" ;
