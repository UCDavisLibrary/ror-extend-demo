cip:13.1501  rdf:type       skos:Concept ;
        skos:prefLabel      "Teacher Assistant/Aide."@en ;
        skos:notation       "13.1501" ;
        skos:broader        cip:13.15 ;
        schema:description  "A program that prepares individuals to assist a teacher in regular classroom settings or in providing instruction and supervision to special student populations, such as bilingual/bicultural students, special education students, adult learners, and students learning English.  Includes instruction in techniques of general classroom supervision, maintaining order, assisting with lessons and carrying out related assignments."@en .

cip:13.1502  rdf:type       skos:Concept ;
